{
  "name": "spaciiing",
  "version": "1.0.0",
  "description": "An all in one figma plugin that enhance your figma editor experience.",
  "main": "code.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "REACT_APP_ENV=development webpack --mode production && npm run inline",
    "freebuild": "REACT_APP_ENV=developmentfree webpack --mode production && npm run inline",
    "production": "REACT_APP_ENV=production webpack --mode production && npm run inline",
    "newProduction": "REACT_APP_ENV=production webpack --mode production",
    "lint": "eslint --ext .ts,.tsx --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .ts,.tsx --ignore-pattern node_modules --fix .",
    "watch": "npm run build -- --watch",
    "inline": "node inlineAssets.js",
    "copyLocale": "node copyLocale.js"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "*",
    "@figma/plugin-typings": "*",
    "@svgr/webpack": "^8.1.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.13.0",
    "license-checker": "^25.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.6.3"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@figma/figma-plugins/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@monaco-editor/react": "^4.6.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-i18next": "^8.1.0",
    "axios": "^1.7.7",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "elkjs": "^0.10.0",
    "html-inline-css-webpack-plugin": "^1.11.2",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.6.3",
    "i18next": "^23.16.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "mini-css-extract-plugin": "^2.9.2",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "uuid": "^11.0.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
